{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [
            true,
            "never"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "no-console": false,
        "interface-name": false,
        "import-name": [
            false
        ],
        "function-name": [
            false
        ],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}